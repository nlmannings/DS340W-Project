s.Input(shape=input_shape)\n","\n","    # Conv layer 1 (follows parent paper: 4 filters)\n","    x = layers.Conv2D(\n","        filters=4,\n","        kernel_size=kernel_size_1,\n","        strides=(2,2),\n","        padding='same',\n","        activation='relu'\n","    )(inp)\n","    x = layers.MaxPool2D(pool_size=(2,2))(x)\n","    x = layers.Dropout(dropout_p)(x)\n","\n","    # Conv layer 2 (8 filters)\n","    # In the parent paper they used L2 on second conv weights; we mimic that but keep it optional here\n","    x = layers.Conv2D(\n","        filters=8,\n","        kernel_size=kernel_size_2,\n","        strides=(2,2),\n","        padding='same',\n","        activation='relu',\n","        kernel_regularizer=regularizers.l2(0.01)\n","    )(x)\n","    x = layers.GlobalMaxPool2D()(x)\n","    x = layers.Dropout(dropout_p)(x)\n","\n","    # Dense head\n","    x = layers.Dense(50, activation='relu')(x)\n","    out = layers.Dense(num_classes, activation='softmax')(x)\n","\n","    model = models.Model(inputs=inp, outputs=out)\n","    model.compile(\n","        optimizer=optimizers.Adam(),\n","        loss='categorical_crossentropy',\n","        metrics=['accuracy']\n","    )\n","    return model\n"],"metadata":{"id":"pz4jyePV7zm4","executionInfo":{"status":"ok","timestamp":1763237366649,"user_tz":300,"elapsed":51,"user":{"displayName":"Nathan Mannings","userId":"04350019282715707776"}}},"execution_count":7,"outputs":[]},{"cell_type":"code","source":["\n","# ---------------------------\n","# 5. Grid search loop (kernel size combinations)\n","# ---------------------------\n","import itertools\n","results = []\n","\n","total_runs = len(KERNEL_SIZES) * len(KERNEL_SIZES)\n","run_idx = 0\n","\n","for ks1, ks2 in itertools.product(KERNEL_SIZES, KERNEL_SIZES):\n","    run_idx += 1\n","    print(f\"\\n=== Run {run_idx}/{total_runs}: kernel1={ks1}, kernel2={ks2} ===\")\n","\n","    model = build_model(kernel_size_1=ks1, kernel_size_2=ks2, input_shape=X_train.shape[1:], dropout_p=DROPOUT_P)\n","\n","    # Callbacks: early stopping (optional small patience)\n","    es = callbacks.EarlyStopping(monitor='val_loss', patience=5, restore_best_weights=True, verbose=1)\n","\n","    history = model.fit(\n","        X_train, y_train_cat,\n","        validation_data=(X_val, y_val_cat),\n","        epochs=EPOCHS,\n","        batch_size=BATCH_SIZE,\n","        callbacks=[es],\n","        verbose=1\n","    )\n","\n","    # Predictions on validation set\n","    y_pred_proba = model.predict(X_val)\n","    y_pred_labels = np.argmax(y_pred_proba, axis=1)\n","    y_true_labels = y_val\n","\n","    bal_acc = balanced_accuracy_score(y_true_labels, y_pred_labels)\n","    print(f\"Balanced accuracy (val): {bal_acc:.4f}\")\n","\n","    results.append({\n","        \"kernel_size_conv1\": f\"{ks1[0]}x{ks1[1]}\",\n","        \"kernel_size_conv2\": f\"{ks2[0]}x{ks2[1]}\",\n","        \"balanced_accuracy\": float(bal_acc),\n","        \"train_epochs_run\": int(len(history.history['loss']))\n","    })\n","\n","# ---------------------------\n","# 6. Save results to CSV and print sorted summary\n","# ---------------------------\n","df = pd.DataFrame(results)\n","df = df.sort_values(\"balanced_accuracy\", ascending=False).reset_index(drop=True)\n","df.to_csv(RESULTS_CSV, index=False)\n","print(f\"\\nGrid search complete. Results saved to {RESULTS_CSV}\\n\")\n","print(df)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"TwbwzjiG73vG","executionInfo":{"status":"ok","timestamp":1763239300285,"user_tz":300,"elapsed":1924905,"user":{"displayName":"Nathan Mannings","userId":"04350019282715707776"}},"outputId":"b66ac92c-88d8-4eaf-9ec0-48ca021f8ac0"},"execution_count":8,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","=== Run 1/9: kernel1=(3, 3), kernel2=(3, 3) ===\n","Epoch 1/10\n","\u001b[1m103/103\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m14s\u001b[0m 111ms/step - accuracy: 0.5334 - loss: 0.7359 - val_accurac